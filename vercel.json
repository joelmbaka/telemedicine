{
  "version": 2,
  "routes": [
    {
      "src": "/appointments/?(.*)",
      "dest": "/app",
      "continue": true
    },
    {
      "src": "/app/?(.*)",
      "dest": "/app",
      "continue": true
    },
    {
      "src": "/_next/static/(.*)",
      "dest": "/_next/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/app"
    }
  ]
}
